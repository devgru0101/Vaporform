    "@opentelemetry/instrumentation": "^0.46.0",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@sentry/node": "^7.81.1",
    "@sentry/tracing": "^7.81.1",
    "@simplewebauthn/server": "^9.0.3",
    "@types/node": "^20.0.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "encore.dev": "^1.49.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-2fa": "^2.0.3",
    "node-schedule": "^2.1.1",
    "node-vault": "^0.10.2",
    "otplib": "^12.0.1",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "typescript": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node-schedule": "^2.1.4",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
