{
  "name": "vaporform-backend",
  "version": "1.0.0",
  "description": "Vaporform AI-powered development environment backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "encore run",
    "build": "encore build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist coverage .tsbuildinfo",
    "migrate": "echo 'Database migration placeholder'",
    "seed": "echo 'Database seeding placeholder'"
  },
  "dependencies": {
    "encore.dev": "^1.30.0",
    "@anthropic-ai/sdk": "^0.24.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.14.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "jest-junit": "^16.0.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "vaporform",
    "ai",
    "development",
    "encore",
    "typescript"
  ],
  "author": "Vaporform Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}