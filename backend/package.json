    "@vitest/ui": "^1.2.0",
    "axe-core": "^4.8.3",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "playwright": "^1.40.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "vaporform",
    "ai",
    "development",
    "encore",
    "typescript"
