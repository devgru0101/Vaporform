{
  "name": "vaporform-backend",
  "version": "1.0.0",
  "description": "Vaporform AI-powered development environment - Pure Encore.ts backend",
  "main": "main.ts",
  "scripts": {
    "dev": "encore run",
    "build": "encore build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "start": "encore run",
    "start:prod": "NODE_ENV=production encore run",
    "type-check": "tsc --noEmit",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "clean": "rimraf dist coverage .tsbuildinfo"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
